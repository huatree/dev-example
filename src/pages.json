{
    "easycom": {
        "^ht-(.*)": "ht-view/components/ht-$1/ht-$1.vue"
    },
    "pages": [
        //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
        {
            "path": "pages/index/index",
            "style": {
                "navigationBarTitleText": "uni-app"
            }
        },
        {
            "path": "pages/sticky/index",
            "style": {
                "navigationBarTitleText": "sticky"
            }
        },
        {
            "path": "pages/highlightText/index",
            "style": {
                "navigationBarTitleText": "highlightText"
            }
        },
        {
            "path": "pages/countdown/index",
            "style": {
                "navigationBarTitleText": "countdown"
            }
        }
    ],
    "globalStyle": {
        "navigationBarTextStyle": "black",
        "navigationBarTitleText": "uni-app",
        "navigationBarBackgroundColor": "#F8F8F8",
        "backgroundColor": "#F8F8F8"
    }
}
